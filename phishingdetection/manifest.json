{
  "manifest_version": 3,
  "name": "URL Safety Checker",
  "version": "1.0",
  "description": "Checks URLs for potential vulnerabilities",
  "permissions": [
    "webNavigation",
    "storage",
    "notifications",
    "scripting",
    "activeTab"
  ],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  